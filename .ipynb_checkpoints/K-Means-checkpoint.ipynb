    "from sklearn.cluster import KMeans"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 28,
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_clusters(points,k):\n",
    "    kmeans = KMeans(n_clusters=2, random_state=0).fit(points)\n",
    "    n=kmeans.labels_\n",
    "    clusters=[[]for i in range(k)]\n",
    "    for i in range(len(n)):\n",
    "        clusters[n[i]].append(points[i])\n",
    "    return clusters"
   ]
  }
